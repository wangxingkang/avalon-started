{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    [
      "es2015",
      {
        "loose": true
      }
    ],
    "stage-2"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": false,
        "regenerator": false,
        "moduleName": "babel-runtime"
      }
    ]
  ],
  "comments": false
}
